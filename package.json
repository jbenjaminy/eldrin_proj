    "babel-loader": "^6.2.7",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "body-parser": "^1.17.0",
    "events": "^2.0.0",
    "express": "^4.14.0",
    "isomorphic-fetch": "^2.2.1",
    "knex": "^0.14.6",
    "lodash": "^4.17.4",
    "pg": "^7.4.1",
    "react": "^15.3.2",
